{
  "name": "rest-api-boilerplate",
  "version": "0.0.0",
  "description": "Node + Express API with JWT auth module",
  "main": "index.js",
  "dependencies": {
    "babel-runtime": "~6.26.0",
    "bcryptjs": "~2.4.3",
    "body-parser": "~1.18.3",
    "express": "~4.16.3",
    "jsonwebtoken": "~8.3.0",
    "mongoose": "~5.1.6"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "~1.7.0",
    "chai": "~4.1.2",
    "eslint": "~4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "jsdoc": "~3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "~5.2.0",
    "nodemon": "~1.17.5"
  },
  "scripts": {
    "babel": "babel src --out-dir dist",
    "build": "npm run clean && npm run babel",
    "clean": "rm -rf node-modules",
    "docs": "jsdoc2md --private -t config/jsdoc2md/README.hbs src/**/**/**/*.js > README.md; echo",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "start": "PORT=3030 nodemon src/ --exec babel-node",
    "serve": "PORT=8080 node dist/index.js",
    "test": "mocha --compilers js:babel-register test/**/*.js",
    "test:watch": "mocha --compilers js:babel-register test/**/*.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prxg22/rest-api-boilerplate.git"
  },
  "author": "prxg22",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prxg22/rest-api-boilerplate/issues"
  },
  "homepage": "https://github.com/prxg22/rest-api-boilerplate#readme"
}
